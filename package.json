{
  "name": "conference-text",
  "version": "0.1.0",
  "description": "Allows one user to manage a group via SMS",
  "scripts": {
    "start": "node ./bin/www",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "mongoose": "^5.4.7",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BandwidthExamples/conference-text.git"
  },
  "keywords": [
    "bandwidth",
    "node",
    "sms",
    "conference",
    "mms",
    "group"
  ],
  "author": "Dan Tolbert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BandwidthExamples/conference-text/issues"
  },
  "homepage": "https://github.com/BandwidthExamples/conference-text#readme"
}
